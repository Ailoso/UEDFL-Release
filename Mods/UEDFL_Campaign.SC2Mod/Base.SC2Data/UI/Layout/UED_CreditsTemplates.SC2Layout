        <Anchor side="Bottom" relative="$parent" pos="Max" offset="-25"/>
        <Anchor side="Right" relative="$parent" pos="Max" offset="-170"/>

        <Width val="340"/>
        <Height val="76"/>
        <Visible val="False"/>
