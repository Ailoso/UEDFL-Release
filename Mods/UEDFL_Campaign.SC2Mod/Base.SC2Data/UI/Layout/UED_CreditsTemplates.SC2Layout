        <Frame type="Image" name="UEDLogo">
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Top" relative="$parent" pos="Min" offset="450"/>

            <Texture val="Assets\Textures\ui_ued_glues_landinglogo1.dds"/>
            <Alpha val="0"/>

            <Animation name="FadeIn">
                <Event event="OnShown" action="Reset,Play" frame="$this"/>

                <Controller type="Fade" end="Pause" frame="$this">
                    <Key type="Curve" time="0.0" value="0" inout="Slow"/>
                    <Key type="Curve" time="0.5" value="0" inout="Slow"/>
                    <Key type="Curve" time="1.5" value="255" inout="Slow"/>
                </Controller>

                <Controller type="Anchor" frame="$this" side="Top" end="Pause" relative="True">
                    <Key type="Curve" out="Slow" time="0" value="0"/>
                    <Key type="Curve" out="Slow" time="6" value="0"/>
                    <Key type="Curve" in="Slow" time="#ScrollDuration" value="#ScrollHeight"/>
                </Controller>
            </Animation>
        </Frame>

